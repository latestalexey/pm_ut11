	ПараметрыФормы = Новый Структура(
			"АнализируемыйСегмент, СформироватьПриОткрытии",
			ПараметрКоманды, Истина
		);
	ОткрытьФорму(
		"Отчет.ПересеченияСегментаНоменклатуры.Форма",
		ПараметрыФормы,
		ПараметрыВыполненияКоманды.Источник,
		ПараметрыВыполненияКоманды.Уникальность,
		ПараметрыВыполненияКоманды.Окно
	);

КонецПроцедуры
