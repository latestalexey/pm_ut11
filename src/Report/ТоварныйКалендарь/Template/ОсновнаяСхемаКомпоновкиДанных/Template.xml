		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗЛИЧНЫЕ
	Сегменты.Номенклатура,
	Сегменты.Характеристика,
	ИСТИНА КАК ИспользуетсяОтборПоСегментуНоменклатуры
ПОМЕСТИТЬ ОтборПоСегментуНоменклатуры
ИЗ
	РегистрСведений.НоменклатураСегмента КАК Сегменты
ИНДЕКСИРОВАТЬ ПО
	Номенклатура,
	Характеристика,
	ИспользуетсяОтборПоСегментуНоменклатуры
{ГДЕ
	Сегменты.Сегмент.* КАК СегментНоменклатуры,
	Сегменты.Номенклатура.* КАК Номенклатура,
	Сегменты.Характеристика.* КАК Характеристика}
;

////////////////////////////////////////////////////////////////////////////////

ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Таблица.Период         КАК Период,
	Таблица.Номенклатура   КАК Номенклатура,
	Таблица.Характеристика КАК Характеристика,
	Таблица.Склад          КАК Склад,

	Таблица.НачальныйОстаток + ЕСТЬNULL(Остатки.ВНаличииОстаток, 0) КАК НачальныйОстаток,
	Таблица.КоличествоПриход КАК КоличествоПриход,
	Таблица.КоличествоРасход КАК КоличествоРасход,
	Таблица.КонечныйОстаток + ЕСТЬNULL(Остатки.ВНаличииОстаток, 0) КАК КонечныйОстаток,
	ВЫБОР
		КОГДА Таблица.Регистратор = НЕОПРЕДЕЛЕНО
			ТОГДА NULL
		ИНАЧЕ Таблица.Регистратор
	КОНЕЦ КАК Регистратор,
	Таблица.ПериодСекунда,
	0					   КАК ПросроченныйПриход,
	0					   КАК ПросроченныйРасход,
	0					   КАК ВНаличии
ИЗ
	(ВЫБРАТЬ
		Таблица.Период                         КАК Период,
		Таблица.Номенклатура                   КАК Номенклатура,
		Таблица.Характеристика                 КАК Характеристика,
		Таблица.Склад                          КАК Склад,
		СУММА(Таблица.НачальныйОстаток)        КАК НачальныйОстаток,
		СУММА(Таблица.КоличествоПриход)        КАК КоличествоПриход,
		СУММА(Таблица.КоличествоРасход)        КАК КоличествоРасход,
		СУММА(Таблица.КонечныйОстаток)         КАК КонечныйОстаток,
		Таблица.Регистратор                    КАК Регистратор,
		МАКСИМУМ(Таблица.ОбъединениеВыполнено) КАК ОбъединениеВыполнено,
		Таблица.ПериодСекунда КАК ПериодСекунда
	ИЗ
		(ВЫБРАТЬ
			НАЧАЛОПЕРИОДА(График.ПериодСекунда, ДЕНЬ) КАК Период,
			ЛОЖЬ                              КАК ОбъединениеВыполнено,
			График.Номенклатура               КАК Номенклатура,
			График.Характеристика             КАК Характеристика,
			График.Склад                      КАК Склад,
			График.КоличествоНачальныйОстаток КАК НачальныйОстаток,
			График.КоличествоКонечныйОстаток  КАК КонечныйОстаток,
			График.КоличествоПриход           КАК КоличествоПриход,
			График.КоличествоРасход           КАК КоличествоРасход,
			График.Регистратор                КАК Регистратор,
			График.ПериодСекунда              КАК ПериодСекунда
		{ВЫБРАТЬ
			Регистратор,
			ПериодСекунда}
		ИЗ
			РегистрНакопления.ГрафикДвиженияТоваров.ОстаткиИОбороты(
					{&amp;ТекущаяДата},
					 , Авто, ДвиженияИГраницыПериода,
					 
				{((Номенклатура, Характеристика) В
				    (ВЫБРАТЬ
				        ОтборПоСегментуНоменклатуры.Номенклатура,
				        ОтборПоСегментуНоменклатуры.Характеристика
				    ИЗ
				        ОтборПоСегментуНоменклатуры
				    ГДЕ
				        ОтборПоСегментуНоменклатуры.ИспользуетсяОтборПоСегментуНоменклатуры = &amp;ИспользуетсяОтборПоСегментуНоменклатуры))}
			) КАК График
		
		ОБЪЕДИНИТЬ ВСЕ
		
		ВЫБРАТЬ
			&amp;ТекущаяДата,
			ИСТИНА,
			Остатки.Номенклатура,
			Остатки.Характеристика,
			Остатки.Склад,
			Остатки.ВНаличииОстаток,
			Остатки.ВНаличииОстаток,
			0,
			0,
			НЕОПРЕДЕЛЕНО,
			NULL
		ИЗ
			РегистрНакопления.СвободныеОстатки.Остатки({&amp;ПустаяДата},
			
				{((Номенклатура, Характеристика) В
				    (ВЫБРАТЬ
				        ОтборПоСегментуНоменклатуры.Номенклатура,
				        ОтборПоСегментуНоменклатуры.Характеристика
				    ИЗ
				        ОтборПоСегментуНоменклатуры
				    ГДЕ
				        ОтборПоСегментуНоменклатуры.ИспользуетсяОтборПоСегментуНоменклатуры = &amp;ИспользуетсяОтборПоСегментуНоменклатуры))}
				        
				) КАК Остатки					
				
			) КАК Таблица
	
	СГРУППИРОВАТЬ ПО
		Таблица.Характеристика,
		Таблица.Период,
		Таблица.Склад,
		Таблица.Номенклатура,
		Таблица.Регистратор,
		Таблица.ПериодСекунда) КАК Таблица
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.СвободныеОстатки.Остатки({&amp;ПустаяДата},
		
				{((Номенклатура, Характеристика) В
				    (ВЫБРАТЬ
				        ОтборПоСегментуНоменклатуры.Номенклатура,
				        ОтборПоСегментуНоменклатуры.Характеристика
				    ИЗ
				        ОтборПоСегментуНоменклатуры
				    ГДЕ
				        ОтборПоСегментуНоменклатуры.ИспользуетсяОтборПоСегментуНоменклатуры = &amp;ИспользуетсяОтборПоСегментуНоменклатуры))}

		) КАК Остатки
		ПО Таблица.Номенклатура = Остатки.Номенклатура
			И Таблица.Характеристика = Остатки.Характеристика
			И Таблица.Склад = Остатки.Склад
			И ((НЕ Таблица.ОбъединениеВыполнено))
			
	ОБЪЕДИНИТЬ ВСЕ
		
	ВЫБРАТЬ
		&amp;ТекущаяДата,
		ПросроченныйГрафик.Номенклатура,
		ПросроченныйГрафик.Характеристика,
		ПросроченныйГрафик.Склад,
		0,
		0,
		0,
		0,
		ПросроченныйГрафик.Регистратор,
		ПросроченныйГрафик.ПериодСекунда,
		ПросроченныйГрафик.КоличествоПриход,
		ПросроченныйГрафик.КоличествоРасход,
		0
	ИЗ
		РегистрНакопления.ГрафикДвиженияТоваров.Обороты(
				, {КОНЕЦПЕРИОДА(ДОБАВИТЬКДАТЕ(&amp;ТекущаяДата, ДЕНЬ, -1), ДЕНЬ)}
				, Авто,
				
				{((Номенклатура, Характеристика) В
			    (ВЫБРАТЬ
			        ОтборПоСегментуНоменклатуры.Номенклатура,
			        ОтборПоСегментуНоменклатуры.Характеристика
			    ИЗ
			        ОтборПоСегментуНоменклатуры
			    ГДЕ
			        ОтборПоСегментуНоменклатуры.ИспользуетсяОтборПоСегментуНоменклатуры = &amp;ИспользуетсяОтборПоСегментуНоменклатуры))}
		) КАК ПросроченныйГрафик			
	
	ОБЪЕДИНИТЬ ВСЕ
		
	ВЫБРАТЬ
		&amp;ТекущаяДата,
		ТекущиеОстатки.Номенклатура,
		ТекущиеОстатки.Характеристика,
		ТекущиеОстатки.Склад,
		0,
		0,
		0,
		0,
		НЕОПРЕДЕЛЕНО,
		NULL,
		0,
		0,
		ТекущиеОстатки.ВНаличииОстаток
	ИЗ
		РегистрНакопления.СвободныеОстатки.Остатки({&amp;ПустаяДата},
		
			{((Номенклатура, Характеристика) В
			    (ВЫБРАТЬ
			        ОтборПоСегментуНоменклатуры.Номенклатура,
			        ОтборПоСегментуНоменклатуры.Характеристика
			    ИЗ
			        ОтборПоСегментуНоменклатуры
			    ГДЕ
			        ОтборПоСегментуНоменклатуры.ИспользуетсяОтборПоСегментуНоменклатуры = &amp;ИспользуетсяОтборПоСегментуНоменклатуры))}
			        
			) КАК ТекущиеОстатки</query>
	</dataSet>
	<totalField>
		<dataPath>КоличествоПриход</dataPath>
		<expression>Сумма(КоличествоПриход)</expression>
