		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ РАЗЛИЧНЫЕ
	ЗаказыДополнительныеРеквизиты.Ссылка КАК ЗаказСсылка,
	ЗаказыДополнительныеРеквизиты.Значение КАК НомерЗаказаНаСайте,
	ЗаказыДополнительныеРеквизитыДаты.Значение КАК ДатаЗаказаНаСайте
ПОМЕСТИТЬ ВремЗаказы
ИЗ
	Документ.ЗаказКлиента.ДополнительныеРеквизиты КАК ЗаказыДополнительныеРеквизиты
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ЗаказКлиента.ДополнительныеРеквизиты КАК ЗаказыДополнительныеРеквизитыДаты
		ПО (ЗаказыДополнительныеРеквизитыДаты.Ссылка = ЗаказыДополнительныеРеквизиты.Ссылка)
			И (ЗаказыДополнительныеРеквизитыДаты.Свойство = &amp;ДопРеквизитДатаЗаказаНаСайте)
ГДЕ
	(НЕ ЗаказыДополнительныеРеквизиты.Ссылка.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаказовКлиентов.Закрыт))
	И ЗаказыДополнительныеРеквизиты.Свойство = &amp;ДопРеквизитНомерЗаказаНаСайте
	И (НЕ ЗаказыДополнительныеРеквизиты.Значение ПОДОБНО "")
{ГДЕ
	ЗаказыДополнительныеРеквизиты.Ссылка.* КАК ЗаказКлиента}
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	МАКСИМУМ(РасчетыСКлиентами.Регистратор) КАК ДокументОтгрузки,
	РасчетыСКлиентами.ЗаказКлиента КАК ЗаказКлиента
ПОМЕСТИТЬ ВремОтгрузки
ИЗ
	ВремЗаказы КАК ВремЗаказы
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрНакопления.РасчетыСКлиентами КАК РасчетыСКлиентами
		ПО ВремЗаказы.ЗаказСсылка = РасчетыСКлиентами.ЗаказКлиента
			И (РасчетыСКлиентами.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход))

СГРУППИРОВАТЬ ПО
	РасчетыСКлиентами.ЗаказКлиента
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
