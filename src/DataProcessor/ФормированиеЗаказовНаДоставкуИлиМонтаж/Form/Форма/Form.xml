			</Type>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>ВЫбрать РАЗЛИЧНЫЕ
 	ВТ_ЗаказыКлиентаТовары.Ссылка,
	 ВТ_ЗаказыКлиентаТовары.Партнер,
	ВТ_ЗаказыКлиентаТовары.Номер,
	ВТ_ЗаказыКлиентаТовары.АК_ПунктНазначения	
ИЗ (ВЫБРАТЬ
	ЗаказКлиента.Ссылка КАК Ссылка,
	ЗаказКлиента.Номенклатура КАК Номенклатура,
	ЗаказКлиента.Характеристика КАК Характеристика,
	ЗаказКлиента.Количество КАК Количество,
   	ЗаказКлиентаОтбор.Партнер,
	ЗаказКлиентаОтбор.Номер,     
	ЗаказКлиентаОтбор.АК_ПунктНазначения
ИЗ
	Документ.ЗаказКлиента.Товары КАК ЗаказКлиента
ПРАВОЕ СОЕДИНЕНИЕ ( ВЫБРАТЬ 
				ЗаказКлиентаДок.Ссылка,
				ЗаказКлиентаДок.Партнер,
				ЗаказКлиентаДок.Номер,
				ЗаказКлиентаДок.АК_ПунктНазначения
				
						
			ИЗ
				Документ.ЗаказКлиента КАК ЗаказКлиентаДок
				ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ  
							Документ.ЗаказКлиента.АК_УслугиДополнительные.Номенклатура ,
							Документ.ЗаказКлиента.АК_УслугиДополнительные.Ссылка
						ИЗ
							Документ.ЗаказКлиента.АК_УслугиДополнительные ) КАК ДопУслугиЗаказКлиента
				ПО ЗаказКлиентаДок.Ссылка  =  ДопУслугиЗаказКлиента.Ссылка 	
			ГДЕ
				ЗаказКлиентаДок.Проведен = ИСТИНА
				И ЗаказКлиентаДок.ПометкаУдаления = ЛОЖЬ
				И ЗаказКлиентаДок.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыЗаказовКлиентов.КОтгрузке)
				И ДопУслугиЗаказКлиента.Номенклатура = ЗНАЧЕНИЕ(Справочник.Номенклатура.АК_Доставка)
) КАК ЗаказКлиентаОтбор   
ПО
ЗаказКлиента.Ссылка =  ЗаказКлиентаОтбор.Ссылка)  КАК ВТ_ЗаказыКлиентаТовары

ЛЕВОЕ СОЕДИНЕНИЕ (			
			
ВЫБРАТЬ
	ЗаказПоставщикуДополнительныеУслуги.Ссылка КАК Ссылка,
	ЗаказПоставщикуДополнительныеУслуги.ЗАказКЛиента КАК ЗаказКлиента,
	ЗаказПоставщикуДополнительныеУслуги.Количество КАК Количество,
	ЗаказПоставщикуДополнительныеУслуги.Номенклатура КАК Номенклатура,
	ЗаказПоставщикуДополнительныеУслуги.Характеристика КАК Характеристика 
ИЗ
	Документ.ЗаказПоставщику.ДополнительныеУслуги КАК ЗаказПоставщикуДополнительныеУслуги,
	
	ПРАВОЕ СОЕДИНЕНИЕ (  ВЫБРАТЬ
				ЗаказПоставщику.Ссылка
			ИЗ
				Документ.ЗаказПоставщику КАК ЗаказПоставщику
			ГДЕ
				ЗаказПоставщику.АК_ТипУслуги = ЗНАЧЕНИЕ(Перечисление.АК_ТипУслуги.Доставка)
				И ЗаказПоставщику.ПометкаУдаления = ЛОЖЬ
				И ЗаказПоставщику.Проведен = ИСТИНА ) КАК ЗаказПоставщикуОтбор   
ПО
ЗаказПоставщикуДополнительныеУслуги.Ссылка =  ЗаказПоставщикуОтбор.Ссылка
)  КАК ВТ_ЗаказПоставщикуУслуги

ПО  ВТ_ЗаказыКлиентаТовары.Ссылка = ВТ_ЗаказПоставщикуУслуги.ЗаказКлиента
        		И  ВТ_ЗаказыКлиентаТовары.Номенклатура =  ВТ_ЗаказПоставщикуУслуги.Номенклатура
		И  ВТ_ЗаказыКлиентаТовары.Характеристика =  ВТ_ЗаказПоставщикуУслуги.Характеристика
ГДЕ ЕСТЬNULL(ВТ_ЗаказыКлиентаТовары.Количество,0)-ЕСТЬNULL (ВТ_ЗаказПоставщикуУслуги.Количество,0)&gt;0
</QueryText>
