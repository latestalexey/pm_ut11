				<Field>Список.ТипОтчета</Field>
				<Field>1/63</Field>
				<Field>Список.ИмяОтчета</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Данные.Ссылка,
	Данные.ВерсияДанных,
	Данные.ПометкаУдаления,
	Данные.Пользовательский,
	НЕ Данные.Пользовательский КАК Предопределенный,
	Данные.Наименование,
	Данные.ВидимостьПоУмолчанию,
	Данные.Отчет,
	Данные.КлючВарианта,
	Данные.ТипОтчета,
	Данные.Автор,
	Данные.Описание,
	ВЫБОР
		КОГДА Данные.ТипОтчета = &amp;ТипВнутренний
			ТОГДА Данные.Отчет.Имя
		КОГДА Данные.ТипОтчета = &amp;ТипДополнительный
			ТОГДА Данные.Отчет
		ИНАЧЕ ПОДСТРОКА(ВЫРАЗИТЬ(Данные.Отчет КАК СТРОКА(150)), 14, 137)
	КОНЕЦ КАК ИмяОтчета,
	ВЫБОР
		КОГДА Данные.ПометкаУдаления = ИСТИНА
			ТОГДА 4
		КОГДА Данные.Пользовательский = ЛОЖЬ
			ТОГДА 5
		ИНАЧЕ 3
	КОНЕЦ КАК ИндексКартинки
ИЗ
	Справочник.ВариантыОтчетов КАК Данные
ГДЕ
	(&amp;РазделИлиГруппа = &amp;КорневаяПодсистема
			ИЛИ 1 В
				(ВЫБРАТЬ ПЕРВЫЕ 1
					1
				ИЗ
					Справочник.ВариантыОтчетов.Размещение КАК ДанныеРазмещения
				ГДЕ
					ДанныеРазмещения.Ссылка = Данные.Ссылка
					И ДанныеРазмещения.Использование = ИСТИНА
					И ВЫБОР
						КОГДА &amp;ВключаяПодчиненные
							ТОГДА ДанныеРазмещения.РазделИлиГруппа В ИЕРАРХИИ (&amp;РазделИлиГруппа)
						ИНАЧЕ ДанныеРазмещения.РазделИлиГруппа = &amp;РазделИлиГруппа
					КОНЕЦ))
	И (&amp;ПоказыватьВсеОтчеты = ИСТИНА
			ИЛИ Данные.Ссылка.Отчет В (&amp;ОтчетыПользователя))
	И (&amp;ОтборНаименование = ""
			ИЛИ Данные.Наименование ПОДОБНО "%" + &amp;ОтборНаименование + "%")
	И (&amp;ОтборАвтор = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
			ИЛИ Данные.Автор = &amp;ОтборАвтор)
	И ВЫБОР
			КОГДА &amp;ОтборТипОтчета = НЕОПРЕДЕЛЕНО
				ТОГДА ИСТИНА
			КОГДА &amp;ОтборТипОтчета = 1
				ТОГДА Данные.ТипОтчета = &amp;ТипВнутренний
						ИЛИ Данные.ТипОтчета = &amp;ТипДополнительный
			ИНАЧЕ Данные.ТипОтчета = &amp;ОтборТипОтчета
		КОНЕЦ</QueryText>
				<MainTable>Catalog.ВариантыОтчетов</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
				</ListSettings>
			</Settings>
		</Attribute>
