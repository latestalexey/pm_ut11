			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	Заявка.Ссылка,
	Заявка.Номер,
	Заявка.Дата,
	Заявка.Организация,
	Заявка.СуммаДокумента,
	Заявка.Валюта,
	
	ВЫБОР КОГДА Заявка.ХозяйственнаяОперация = ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.СдачаДенежныхСредствВБанк)
		ИЛИ Заявка.ХозяйственнаяОперация = ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ВыдачаДенежныхСредствВДругуюКассу)
	ТОГДА
		Заявка.Касса
	ИНАЧЕ
		Заявка.БанковскийСчет
	КОНЕЦ КАК БанковскийСчетКасса,
	
	ВЫБОР КОГДА Заявка.ХозяйственнаяОперация = ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ВыдачаДенежныхСредствВДругуюКассу)
		ИЛИ Заявка.ХозяйственнаяОперация = ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ПоступлениеДенежныхСредствИзБанка)
	ТОГДА
		Заявка.КассаПолучатель
	ИНАЧЕ
		Заявка.БанковскийСчетПолучатель
	КОНЕЦ КАК БанковскийСчетКассаПолучатель,
	
	Заявка.Статус,
	Заявка.ХозяйственнаяОперация
ИЗ
	Документ.РаспоряжениеНаПеремещениеДенежныхСредств КАК Заявка</QueryText>
