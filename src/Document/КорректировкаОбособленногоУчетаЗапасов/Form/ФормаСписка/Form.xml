			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ДанныеДокумента.Ссылка,
	ДанныеДокумента.ПометкаУдаления,
	ДанныеДокумента.Номер,
	ДанныеДокумента.Дата,
	ДанныеДокумента.Проведен,
	ДанныеДокумента.Организация,
	ДанныеДокумента.Склад,
	ДанныеДокумента.Предназначение,
	ДанныеДокумента.НовоеПредназначение,
	ДанныеДокумента.Комментарий,
	ДанныеДокумента.МоментВремени,
	
	ВЫБОР КОГДА ДанныеДокумента.Предназначение = ЗНАЧЕНИЕ(Перечисление.ТипыПредназначенияВидовЗапасов.ПредназначенДляСделки) ТОГДА
		ДанныеДокумента.Сделка
	КОГДА ДанныеДокумента.Предназначение = ЗНАЧЕНИЕ(Перечисление.ТипыПредназначенияВидовЗапасов.ПредназначенДляПодразделения) ТОГДА
		ДанныеДокумента.Подразделение
	КОГДА ДанныеДокумента.Предназначение = ЗНАЧЕНИЕ(Перечисление.ТипыПредназначенияВидовЗапасов.ПредназначенДляМенеджера) ТОГДА
		ДанныеДокумента.Менеджер
	ИНАЧЕ
		Неопределено
	КОНЕЦ КАК ИсходныеВидыЗапасов,
	
	ВЫБОР КОГДА ДанныеДокумента.НовоеПредназначение = ЗНАЧЕНИЕ(Перечисление.ТипыПредназначенияВидовЗапасов.ПредназначенДляСделки) ТОГДА
		ДанныеДокумента.НоваяСделка
	КОГДА ДанныеДокумента.НовоеПредназначение = ЗНАЧЕНИЕ(Перечисление.ТипыПредназначенияВидовЗапасов.ПредназначенДляПодразделения) ТОГДА
		ДанныеДокумента.НовоеПодразделение
	КОГДА ДанныеДокумента.НовоеПредназначение = ЗНАЧЕНИЕ(Перечисление.ТипыПредназначенияВидовЗапасов.ПредназначенДляМенеджера) ТОГДА
		ДанныеДокумента.НовыйМенеджер
	ИНАЧЕ
		Неопределено
	КОНЕЦ КАК НовыеВидыЗапасов
ИЗ
	Документ.КорректировкаОбособленногоУчетаЗапасов КАК ДанныеДокумента</QueryText>
