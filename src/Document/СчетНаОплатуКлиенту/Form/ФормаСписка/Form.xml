			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	СчетНаОплатуКлиенту.Ссылка                       КАК Ссылка,
	СчетНаОплатуКлиенту.ПометкаУдаления              КАК ПометкаУдаления,
	СчетНаОплатуКлиенту.Номер                        КАК Номер,
	СчетНаОплатуКлиенту.Дата                         КАК Дата,
	СчетНаОплатуКлиенту.Проведен                     КАК Проведен,
	СчетНаОплатуКлиенту.Организация                  КАК Организация,
	СчетНаОплатуКлиенту.Партнер                      КАК Партнер,
	СчетНаОплатуКлиенту.Контрагент                   КАК Контрагент,
	СчетНаОплатуКлиенту.Договор                      КАК Договор,
	СчетНаОплатуКлиенту.ДокументОснование.Сделка     КАК Сделка,
	СчетНаОплатуКлиенту.Валюта                       КАК Валюта,
	СчетНаОплатуКлиенту.ДокументОснование            КАК ДокументОснование,
	СчетНаОплатуКлиенту.ФормаОплаты                  КАК ФормаОплаты,
	СчетНаОплатуКлиенту.БанковскийСчет               КАК БанковскийСчет,
	СчетНаОплатуКлиенту.Касса                        КАК Касса,
	СчетНаОплатуКлиенту.ДополнительнаяИнформация     КАК ДополнительнаяИнформация,
	СчетНаОплатуКлиенту.ЧастичнаяОплата              КАК ЧастичнаяОплата,
	СчетНаОплатуКлиенту.Аннулирован                  КАК Аннулирован,
	СчетНаОплатуКлиенту.СуммаДокумента               КАК СуммаДокумента,
	СчетНаОплатуКлиенту.Комментарий                  КАК Комментарий,
	ВЫБОР
		// Не проведен
		КОГДА
			НЕ СчетНаОплатуКлиенту.Проведен
		ТОГДА
			ЗНАЧЕНИЕ(Перечисление.СостоянияСчетовНаОплатуКлиентам.ПустаяСсылка)
		// Выставлен
		КОГДА
